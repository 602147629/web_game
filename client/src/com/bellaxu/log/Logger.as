package com.bellaxu.log
{
	/**
	 * 日志类
	 * @author BellaXu
	 */
	public class Logger
	{
		public static function info(str:String):void
		{
			trace(str);
		}
		
		public static function warn(str:String):void
		{
			trace(str);
		}
		
		public static function error(str:String):void
		{
			trace(str);
		}
	}
}